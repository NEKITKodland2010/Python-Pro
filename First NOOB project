meme_dict = {
            "КРИНЖ,кринж": "Что-то очень странное или стыдное",
            "ЛОЛ": "Что-то очень смешное",
            "РОФЛ": "Шутка",
            "ЩИЩ": "Одобрение",
            "КРИПОВЫЙ": "Страшный",
            "АГРИТЬСЯ": "Злиться"
            }
word = input("Введите непонятное слово (большими буквами!) : ")
if word in meme_dict.keys():
    print(meme_dict[word])
else:
    print("Я не знаю такое слово")
